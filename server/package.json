{
  "name": "gogedit",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run prebuild && tsc && tsc-alias",
    "create:migration": "npx mikro-orm migration:create",
    "dev": "nodemon",
    "start": "npm run build && node dist/index.js",
    "format": "npx prettier --write 'src/**/*.+(js|ts|json)'",
    "lint": "eslint 'src/**/*.{js,ts}' --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/admhpr/gogedit.git"
  },
  "keywords": [],
  "devDependencies": {
    "@types/connect-redis": "^0.0.18",
    "@types/express": "^4.17.13",
    "@types/graphql": "^14.5.0",
    "@types/node": "^17.0.0",
    "@types/redis": "^4.0.11",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "tsc-alias": "^1.5.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@mikro-orm/cli": "^4.5.9",
    "@mikro-orm/core": "^4.5.9",
    "@mikro-orm/migrations": "^4.5.9",
    "@mikro-orm/postgresql": "^4.5.9",
    "apollo-server-express": "^3.5.0",
    "argon2": "^0.28.3",
    "connect-redis": "^5.0.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-session": "^1.17.2",
    "graphql": "^15.8.0",
    "pg": "^8.7.1",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "urql": "^2.0.6"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ignore": [],
    "ext": "ts,js,json,graphql",
    "exec": "tsc && tsc-alias && node ./dist/index.js"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  }
}
